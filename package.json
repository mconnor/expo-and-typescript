{
    "name": "expo-and-typescript",
    "version": "0.0.1",
    "description": "Showcase of an Expo app coded in TypeScript.",
    "author": {
        "email": "jan@aagaard.net",
        "name": "Jan Aagaard"
    },
    "private": true,
    "license": "MIT",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "clear": "./scripts/clear.sh",
        "android": "yarn run start --android",
        "check-types": "tsc",
        "ios": "yarn run start --ios",
        "lint": "eslint --ext js,ts,tsx './src/**/*'",
        "fix": "eslint --ext js,ts,tsx './src/**/*' --fix",
        "start": "yarn install && expo start",
        "validate": "yarn install --frozen-lockfile && yarn run lint && yarn run check-types"
    },
    "engines": {
        "node": ">=16",
        "yarn": ">=3.1"
    },
    "packageManager": "yarn@3.1.1",
    "dependencies": {
        "@react-navigation/native": "^6.0.6",
        "@react-navigation/stack": "^6.0.11",
        "expo": "^44.0.0",
        "expo-analytics-amplitude": "~11.1.0",
        "expo-apple-authentication": "~4.1.0",
        "expo-asset": "~8.4.6",
        "expo-av": "~10.2.0",
        "expo-barcode-scanner": "~11.2.0",
        "expo-blur": "~11.0.0",
        "expo-camera": "~12.1.0",
        "expo-constants": "~13.0.1",
        "expo-facebook": "~12.1.0",
        "expo-font": "~10.0.4",
        "expo-linear-gradient": "~11.0.0",
        "expo-local-authentication": "~12.1.0",
        "expo-sensors": "~11.1.0",
        "react": "17.0.1",
        "react-native": "0.64.3",
        "react-native-gesture-handler": "~2.1.0",
        "react-native-maps": "0.29.4",
        "react-native-reanimated": "~2.3.1",
        "react-native-safe-area-context": "3.3.2",
        "react-native-screens": "~3.10.1",
        "react-native-svg": "12.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@types/react": "~17.0.21",
        "@types/react-native": "~0.64.12",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "babel-preset-expo": "9.0.2",
        "eslint": "^7.32.0",
        "eslint-config-universe": "^10.0.0",
        "prettier": "2.5.1",
        "typescript": "~4.3.5"
    }
}
